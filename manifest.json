{
  "manifest_version": 2,
  "name": "Search Extension",
  "version": "1.0",
  "description": "Search extension with multiple search engines",
  "icons": {
    "16": "icon16.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "browser_action": {
    "default_icon": "icon48.jpg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "styles.css"
  ]
}
